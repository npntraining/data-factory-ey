{
	"name": "01_IfCondition",
	"properties": {
		"activities": [
			{
				"name": "If Condition1",
				"type": "IfCondition",
				"dependsOn": [],
				"userProperties": [],
				"typeProperties": {
					"expression": {
						"value": "@contains(pipeline().parameters.List_Of_cities,pipeline().parameters.CityToSearch)\n",
						"type": "Expression"
					},
					"ifFalseActivities": [
						{
							"name": "Value Does not Exists",
							"type": "SetVariable",
							"dependsOn": [],
							"policy": {
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"variableName": "iFCityExists",
								"value": "False"
							}
						}
					],
					"ifTrueActivities": [
						{
							"name": "Value Exists",
							"type": "SetVariable",
							"dependsOn": [],
							"policy": {
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"variableName": "iFCityExists",
								"value": "True"
							}
						}
					]
				}
			}
		],
		"parameters": {
			"List_Of_cities": {
				"type": "array",
				"defaultValue": [
					"New York",
					"London",
					"India"
				]
			},
			"CityToSearch": {
				"type": "string",
				"defaultValue": "Mysore"
			}
		},
		"variables": {
			"iFCityExists": {
				"type": "String"
			}
		},
		"folder": {
			"name": "Control Flow activities/01_IF_condition"
		},
		"annotations": []
	}
}