{
	"name": "PK_ADLS_EmployeeDBExport",
	"properties": {
		"linkedServiceName": {
			"referenceName": "LS_AzureDataLakeStorage_PK",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"SchemaNameDS": {
				"type": "string"
			},
			"TableNameDS": {
				"type": "string"
			},
			"FilenameDS": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@dataset().FilenameDS",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@concat('employeedbexport','/',dataset().SchemaNameDS,'/',dataset().TableNameDS)",
					"type": "Expression"
				},
				"fileSystem": "adf-container"
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": []
	}
}